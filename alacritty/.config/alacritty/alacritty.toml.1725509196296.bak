import = [
  "/usr/lib/node_modules/alacritty-themes/themes/Catppuccin-Macchiato.toml"
]
working_directory = "None"
live_config_reload = true
ipc_socket = true

[env]
TERM = "screen-256color"

[window]
position = "None"
dynamic_padding = false
decorations = "Buttonless"
opacity = 1
blur = false
startup_mode = "Windowed"
title = "Alacritty"
dynamic_title = true
decorations_theme_variant = "None"
resize_increments = false
option_as_alt = "Both"

  [window.dimensions]
  columns = 0
  lines = 0

  [window.padding]
  x = 0
  y = 0

  [window.class]
  instance = "Alacritty"
  general = "Alacritty"

[scrolling]
history = 10_000
multiplier = 1

[font]
size = 12
builtin_box_drawing = true

  [font.bold]
  style = "Bold"

  [font.italic]
  style = "Italic"

  [font.bold_italic]
  style = "Bold Italic"

  [font.offset]
  x = 0
  y = 0

  [font.glyph_offset]
  x = 0
  y = 0

[colors]
transparent_background_colors = false
draw_bold_text_with_bright_colors = false
primary = { }
cursor = { }
vi_mode_cursor = { }
line_indicator = { }
footer_bar = { }
selection = { }
normal = { }
bright = { }
dim = { }

  [colors.search]
  matches = { }
  focused_match = { }

  [colors.hints]
  start = { }
  end = { }

[bell]
animation = "Linear"
duration = 0
color = "#ffffff"
command = "None"

[selection]
semantic_escape_chars = ",â”‚`|:\"' ()[]{}<>\\t"
save_to_clipboard = false

[cursor]
vi_mode_style = "None"
blink_interval = 750
blink_timeout = 0
unfocused_hollow = true
thickness = 0.15

  [cursor.style]
  shape = "Block"
  blinking = "On"

[terminal]
osc52 = "CopyPaste"

[mouse]
hide_when_typing = true
bindings = [ ]

[hints]
alphabet = "jfkdls;ahgurieowpq"

[keyboard]
bindings = [ ]
